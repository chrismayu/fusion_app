<%= simple_form_for(@youth) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :phone_number %>
    <%= f.input :birthday %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
